package org.somak.restclient;

import javax.ws.rs.client.Client;
import javax.ws.rs.client.ClientBuilder;
import javax.ws.rs.client.WebTarget;

public class InvokationDemo {
	
	public static void main(String[] args)
	{
		
	}
	
	
	public void prepareRequestForMessageByYear(int yr) {
		
		//url pattern: http://localhost:8080/messenger1/webapi/messages?year=2019
		Client client=ClientBuilder.newClient();
		WebTarget target=client.target("http://localhost:8080/messenger1/webapi/").path("messages").queryParam("year", yr );
		
		
		
	}

}
